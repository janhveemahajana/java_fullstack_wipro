package com.wipro.anonymous;

import java.util.function.Function;

public class FunctionEx9 {

	public static void main(String[] args) {
		Function<Double, Double> fraction = fr -> {double num = fr - Math.floor(fr);
		double fractionres = num - Math.floor(num);
        return String.format("%.2f", fractionres);
        };
		
		double number = 123.45;
        double result = fraction.apply(number);

        System.out.println("Fraction of " + number + " is " + result);
	}

}
