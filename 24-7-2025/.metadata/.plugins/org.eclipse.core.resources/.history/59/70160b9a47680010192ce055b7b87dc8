package com.wipro.collections;

public class ArrayList {

	public static void main(String[] args) {
		ArrayList list = new ArrayList();
		list.add(1);
		list.add("Janhvee");
		list.add(12.3);
		
		System.out.println(list);
		System.out.println(list.size());
		list.remove(1);
		System.out.println(list.size());
	}

}
